{
    "outputDirectory": "api",
    "buildCommand": "npm run prod",
    "installCommand": "npm install",
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.6.1"
        }
    },
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.php"
        }
    ]
}