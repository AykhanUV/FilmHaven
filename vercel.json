{
    "outputDirectory": "site/public/dist",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "routes": [
        {
            "src": "/api(.*)",
            "dest": "/api/src/index"
        }
    ],
    "env": {
        "ORIGIN": "*",
        "CACHE_TIMEOUT": "300",
        "TIMEOUT": "4"
    }
}